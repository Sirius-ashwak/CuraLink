# Industry-Ready Telehealth Platform - Environment Variables
# Copy this file to .env and fill in your actual API keys

# ===== FIREBASE CONFIGURATION =====
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# ===== GOOGLE CLOUD CONFIGURATION =====
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_CLIENT_ID=your_oauth_client_id
GOOGLE_CLOUD_CLIENT_SECRET=your_oauth_client_secret
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key

# ===== AI SERVICES =====
GEMINI_API_KEY=your_gemini_api_key_here

# ===== TWILIO (VIDEO CALLS & SMS) =====
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret

# ===== GOOGLE MAPS =====
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# ===== MAPBOX =====
MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here
MAPBOX_SECRET_KEY=your_mapbox_secret_key_here
VITE_MAPBOX_STYLE_URL=your_custom_mapbox_style_url_here

# ===== HEALTHCARE API SETTINGS =====
GOOGLE_HEALTHCARE_LOCATION=us-central1
GOOGLE_HEALTHCARE_DATASET=telehealth
GOOGLE_HEALTHCARE_FHIR_STORE=telehealth-fhir-store
GOOGLE_HEALTHCARE_DICOM_STORE=telehealth-dicom-store

# ===== APPLICATION SETTINGS =====
NODE_ENV=production
PORT=5000
USE_SECRET_MANAGER=false

# ===== HTTPS CONFIGURATION =====
USE_HTTPS=true
HTTPS_PORT=5443
SSL_KEY_PATH=../ssl/key.pem
SSL_CERT_PATH=../ssl/cert.pem

# ===== SECURITY SETTINGS =====
# Add any additional security configurations here
ENCRYPTION_KEY=your_encryption_key_for_phi_data